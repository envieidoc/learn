    [ -f simple-tftpd ] && mv simple-tftpd "$1" || _die "$(expr "$0" : '.*/\([^/]*\)'): simple-tftpd couldn't be created"
}

_pxe()
{
    if [ -n "$1" ]; then
        [ -d "$1" ] && DEFAULT_PATH="$1" || DEFAULT_PATH="."
        shift
    else
        DEFAULT_PATH="."
    fi
    DEFAULT_PATH="$DEFAULT_PATH"/pxe_setup

    #Find the interface used for the default route, I'm not sure if this is useful or harmful, need testing in 
    #multi-interface environments
    #_pxe_var_interface=${host_ip_iface:-$(ip route | sed -n '/^default/{ s/.*dev \(\w\+\)\s\+.*/\1/; p; }' | head -1)}

    #_pxe_var_interface="$1"

    #if ! _validinterface "$_pxe_var_interface"; then
        #_die "$(expr "$0" : '.*/\([^/]*\)'): $_pxe_var_interface is not a valid interface"
    #fi

    mkdir "$DEFAULT_PATH" 2>/dev/null
    [ -d "$DEFAULT_PATH" ] || _die "pxe:cannot write in $(_dirname "$DEFAULT_PATH")"

    printf "%s\\n" "[+] setting pxe environment in $DEFAULT_PATH ..."
    _deps "$DEFAULT_PATH" #memdisk,menu.c32,pxelinux.0,simple-dhcpd,simple-tftpd
    _writeconf "$DEFAULT_PATH" #pxelinux.cfg/default,ubuntu,fedora,tools

    printf "%s\\n" "[+] done!, have fun!"
}

if [ -n "$1" ]; then
    case "$1" in
        -h|--help) _usage ;;
        *) _pxe_var_default_dir="$1";;
    esac
fi

_header
[ -n "$_pxe_var_default_dir" ] && _pxe "$_pxe_var_default_dir" || _pxe

