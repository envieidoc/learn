#!/usr/bin/env sh
#setup a pxe environment
trap _cleanup 0 15

updates="https://raw.github.com/chilicuil/learn/master/sh/is/pxe"

_usage()
{
    printf "%s\\n" "Usage: $(expr "$0" : '.*/\([^/]*\)') [PATH-TO-DEPLOY]"
    printf "\\n"
    printf "%s\\n" "  path-to-deploy   where to create the pxe environment (default ./)"
    exit 1
}

_header()
{
    printf "%b\\n" "\033[1m--------------------------\033[7m Setup PXE \033[0m\033[1m---------------------------\033[0m"
    printf "%b\\n" "\033[1mUpdates:\033[0m $updates"
    printf "%b\\n" "\033[1m----------------------------------------------------------------\033[0m"
}

_die()
{   #print a stacktrace with a msg, exits with 1
    if [ -n "$bash" ]; then
        _die_var_frame=0
        while caller $_die_var_frame; do
            _die_var_frame=$(expr $_die_var_frame + 1);
        done
    fi

    printf "%s\\n" "$*"
    exit
}

_cleanup()
{
    rm -rf "$DEFAULT_PATH"
}

_dirname()
{   #returns string containing dirname on success, 1 on fail
    [ -z "$1" ] && return 1

    case $1 in
        /*|*/*) #http://www.linuxselfhelp.com/gnu/autoconf/html_chapter/autoconf_10.html
            dir=$(expr "x$1" : 'x\(.*\)/[^/]*' \| '.'      : '.')
            printf "%s\\n" "$dir"
            ;;
        *) printf "%s\\n" ".";;
    esac
}

_whereis()
{
    [ -z "$1" ] && return 1
    [ -n "$2" ] && return 1
    if ! command -v "$1" 2>/dev/null; then
        return 1
    fi
}

_validinterface()
{   #return 0 if parameter is a valid network interface, 1 otherwise
    [ -z "$1" ] && return 1
    ip addr show | grep "$1": >/dev/null && return 0
    return 1
}

_cmd()
{   #print current command, exits on fail
    [ -z "$1" ] && return 1

    printf "%s " "    $ $@"
    printf "\\n"
    eval "$@"

    _cmd_var_status=$?
    [ X"$_cmd_var_status" != X"0" ] && exit $_cmd_var_status || return 0
}

_handscui()
{
    [ -z "$1" ] && { printf "%5s\n" ""; return 1; }
    printf "%s\\n" "$1" | grep -v "[^0-9]" >/dev/null || { printf "%5s\n" ""; return 1; }
    _animcui_var_pid="$1"
    _animcui_var_animation_state=1

    if [ ! "$(ps -p $_animcui_var_pid -o comm=)" ]; then
        printf "%5s\n" ""
        return 1
    fi

    printf "%s" "      "

    while [ "$(ps -p $_animcui_var_pid -o comm=)" ]; do
        printf "%b" "\b\b\b\b\b"
        case $_animcui_var_animation_state in
            1)
                printf "%s" '\o@o\'
                _animcui_var_animation_state=2
                ;;
            2)
                printf "%s" '|o@o|'
                _animcui_var_animation_state=3
                ;;
            3)
                printf "%s" '/o@o/'
                _animcui_var_animation_state=4
                ;;
            4)
                printf "%s" '|o@o|'
                _animcui_var_animation_state=1
                ;;
        esac
        sleep 1
    done
    printf "%b" "\b\b\b\b\b" && printf "%5s\n" ""
}

_waitfor()
{
    [ -z "$1" ] && return 1

    printf "%s " "    $ $@"
    eval "$@" > /dev/null 2>/dev/null &
    sleep 1s

    _handscui $(pidof $1)
}

_writeconf()
{
    [ ! -d "$1" ] && _die "$(expr "$0" : '.*/\([^/]*\)'): No valid directory $1"

    printf "%s\\n" "  - creating "$1"/pxelinux.cfg/default ..."
    mkdir "$1"/pxelinux.cfg 2>/dev/null
    cat > "$1"/pxelinux.cfg/default << EOF
DEFAULT menu.c32
TIMEOUT 500
ONTIMEOUT BootLocal
PROMPT 0
NOESCAPE 1
ALLOWOPTIONS 1
PROMPT 0

MENU TITLE  PXE
MENU WIDTH 80
MENU ROWS 14
MENU TABMSGROW 24
MENU MARGIN 10

NOESCAPE 1

LABEL   Local
        localboot 0
        TEXT HELP
        Boot from local disk
        ENDTEXT
MENU BEGIN Ubuntu
MENU TITLE Ubuntu
        LABEL Previous
        MENU LABEL Previous Menu
        TEXT HELP
        Previous Menu
        ENDTEXT
        MENU EXIT
        MENU SEPARATOR
        MENU INCLUDE ubuntu/ubuntu.menu
MENU END
MENU BEGIN Fedora
MENU TITLE Fedora
        LABEL Previous
MENU LABEL Previous Menu
        TEXT HELP
        Previous Menu
        ENDTEXT
        MENU EXIT
        MENU SEPARATOR
        MENU INCLUDE fedora/fedora.menu
MENU END
MENU BEGIN Tools
MENU TITLE Tools
        LABEL Previous
        MENU LABEL Previous Menu
        TEXT HELP
        Previous Menu
        ENDTEXT
        MENU EXIT
        MENU SEPARATOR
        MENU INCLUDE tools/tools.menu
MENU END
EOF

    printf "%s\\n" "  - creating $1/ubuntu/ubuntu.menu ..."
    mkdir -p "$1"/ubuntu/1204/amd64
    mkdir -p "$1"/ubuntu/1204/i386
    cat > "$1"/ubuntu/ubuntu.menu << EOF
LABEL 1
MENU LABEL Ubuntu minimal 12.04 (32-bit)
KERNEL ubuntu/1204/i386/linux
IPAPPEND 2
APPEND lang=us interface=auto initrd=ubuntu/1204/i386/initrd.gz
TEXT HELP
Install Ubuntu 12.04 (32-bit)
ENDTEXT
LABEL 2
MENU LABEL Ubuntu minimal 12.04 (64-bit)
KERNEL ubuntu/1204/amd64/linux
IPAPPEND 2
APPEND lang=us interface=auto initrd=ubuntu/1204/amd64/initrd.gz
TEXT HELP
Install Ubuntu 12.04 (64-bit)
ENDTEXT
LABEL 3
MENU LABEL Ubuntu minimal 12.04 (32-bit)(hands-free)
KERNEL ubuntu/1204/i386/linux
IPAPPEND 2
APPEND DEBCONF_DEBUG=5 priority=critical locale=en_US interface=auto netcfg/get_nameservers=8.8.8.8 url=http://people.ubuntu.com/~chilicuil/conf/preseed/minimal.preseed initrd=ubuntu/1204/i386/initrd.gz
#APPEND DEBCONF_DEBUG=5 priority=critical locale=en_US interface=auto proxy=http://10.99.88.1:3142 netcfg/get_nameservers=8.8.8.8 url=http://people.ubuntu.com/~chilicuil/conf/preseed/minimal.preseed initrd=ubuntu/1204/i386/initrd.gz
TEXT HELP
Install Ubuntu 12.04 (32-bit)
ENDTEXT
LABEL 4
MENU LABEL Ubuntu minimal 12.04 (64-bit)(hands-free)
KERNEL ubuntu/1204/amd64/linux
IPAPPEND 2
APPEND DEBCONF_DEBUG=5 priority=critical locale=en_US interface=auto netcfg/get_nameservers=8.8.8.8 url=http://people.ubuntu.com/~chilicuil/conf/preseed/minimal.preseed initrd=ubuntu/1204/amd64/initrd.gz
#APPEND DEBCONF_DEBUG=5 priority=critical locale=en_US interface=auto proxy=http://10.99.88.1:3142 netcfg/get_nameservers=8.8.8.8 url=http://people.ubuntu.com/~chilicuil/conf/preseed/minimal.preseed initrd=ubuntu/1204/amd64/initrd.gz
TEXT HELP
Install Ubuntu 12.04 (64-bit)
ENDTEXT
EOF
    printf "%s\\n" "  - creating $1/pxe/fedora/fedora.menu ..."
    mkdir -p "$1"/fedora/19/amd64
    mkdir -p "$1"/fedora/19/i386
    cat > "$1"/fedora/fedora.menu << EOF
LABEL 1
MENU LABEL Fedora minimal 19 (32-bit)
KERNEL fedora/19/i386/linux
APPEND lang=us keymap=us ip=dhcp ksdevice=eth0 initrd=fedora/19/i386/initrd.gz
TEXT HELP
Install Fedora 19 (32-bit)
ENDTEXT
LABEL 2
MENU LABEL Fedora minimal 19 (64-bit)
KERNEL fedora/19/amd64/linux
APPEND lang=us keymap=us ip=dhcp ksdevice=eth0 initrd=fedora/6/amd64/initrd.gz
TEXT HELP
Install Fedora 19 (64-bit)
ENDTEXT
EOF

    printf "%s\\n" "  - creating $1/tools/tools.menu ..."
    mkdir -p "$1"/tools
    touch "$1"/tools/tools.menu
}

_deps()
{
    [ ! -d "$1" ] && _die "$(expr "$0" : '.*/\([^/]*\)'): No valid directory $1"
    _whereis python >/dev/null || _die "$(expr "$0" : '.*/\([^/]*\)'): python was not found!"
    #menu.c32 and pxelinux.0 were taken from https://github.com/psychomario/PyPXE
   
    #python programs are customized version of psychomario's
    #https://github.com/chilicuil/learn/tree/master/python

    #feel free to use your own versions if you don't trust me

    #menu.c32
    printf "%s\\n" "  - creating "$1"/menu.c32 ..."
    cat<<E=O=F | openssl enc -base64 -d | zcat | tar xvf - >/dev/null
