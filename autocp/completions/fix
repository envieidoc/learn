_fix()
{
    #defining local vars
    local cur prev words cword
    _init_completion || return

    COMMANDS="apache ipw2200 wl iwlagn mpd webcam"
    OPTS="-h --help -v --version"

    case "${cur}" in
        -*)
            completions="$OPTS"
            ;;
        *)
            completions="$COMMANDS"
            ;;
    esac

    COMPREPLY=( $( compgen -W "$completions" -- $cur ))
    return 0
} &&
complete -F _fix fix
